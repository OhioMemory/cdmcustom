html { margin-top: 0px !important; }
* html body { margin-top: 32px !important; }
@media screen and ( max-width: 782px ) {
	html { margin-top: 46px !important; }
	* html body { margin-top: 46px !important; }
}
body { 
	font-family: "Open Sans", sans-serif;
    /*font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
    font-size: 15px;
    color: #333;*/
    background-color: #fff;
}
p {
	margin: 0 0 10px;
    font-size: 120%;
}
#main { 
	margin-top: -25px;
	overflow: hidden; 
	padding-right: 60px;
	padding-left: 70px;
}
.onecol .onecol .entry-summary,
.entry-content {
	font-family: "Open Sans", sans-serif;
	font-size:130%;
	line-height:1.62em;
}
#headerarea {
	border-top: none; /* change */
}
#headerarea input#s {
	width:168px;
	box-shadow:inset 1px 1px 5px 1px rgba(0, 0, 0, .1);
	text-indent: 0;
}
#wrapper {
	max-width: 100%; 
}
#background-banner {
	width:100%;
	height:120px; /*12em*/
	background-size: auto;
	background:url('/customizations/global/pages/homepage_resources/banner4.png') no-repeat;
}
#background-banner.sized {
	 background-size: cover;
 	 -moz-background-size: cover;  /* Firefox 3.6 */
	background-position: center;  /* Internet Explorer 7/8 */
}
#site-title {
	/*margin: 0 3.4%;*/
	margin-left: 3.4%;
	margin-right: 1%;
	margin-top: 6px;
}
#site-title a,
#site-description {
	color:#000;
}
@media screen and (max-width: 1000px) {
	#site-description {
		display: none;
	}
}
#site-description {
	
	font-size:138%;
	text-align:center;
	line-height:30px;
	/*width:50%;*/
	word-wrap: break-word;
	margin-top: 24px;
}
#site-description span {
	text-shadow: 1px 1px 1px rgba(150, 150, 150, 1), 1px 1px 6px #fff;
}

#site-title a img {
	width:180px;
	vertical-align: top;
	margin: 0 0;	
}
#advanced {
	float: right;
}
#simple-search {
	display: inline;
}
#text-check {
    display: inline-block;
    clear: both;
    height: 40px;
    width: 300px;
    vertical-align: bottom;
}
#access {
	background-color: black;
}
#access ul {
	margin: 0 auto;
	width: 100%;
	float: none;
}							
#menu-topbar-wrapper {
	margin: 0 auto;
}
#restrict_element {
	margin-top: 10px;
	padding-top: 4px;
}

#browse-contributor {
	display: inline;
}
#browse-place {
	display: inline;
}
#browse-subject {
	display: inline;
}	
.page .entry-title {
	margin-bottom: 0px;
}
.single .entry,
.page .entry {
	padding: 0px 20px 20px 20px;
	margin-bottom:5.1% !important;
}
#footer {
    clear: both;
    border-top: 0px;
}
a:hover {
	color:#aa237a;
}
.widget-title {
	color:#444444;
}
.widget-area a {
	color:#269acd;
}
#copyright a {
	color:#269acd;
}
.search-and-browse {
	text-align:center;
	padding-top:10px;
}
@media (min-width: 768px) {
	.form-inline .form-control {
		width: 30%;
	}
	.CoreLayout-mainWrapperContainer {
    	margin-top: 0px;
	}
}
.CoreLayout-mainwrapper {
    margin-top: 0px;
    background-color: white;
    padding-right: 0px; /* change */
    padding-left: 0px; /* change */
}
.flex-caption {
	padding: 2%;
	left: 0;
	bottom: 0;
	color: #fff;
	font-size: 14px;
	line-height: 18px;
	
}
.caption-title {
	font-size: 18px;
	line-height: 18px;
	color: rgb(240,240,240);
	text-shadow: 2px 2px rgb(50,50,50);
	text-align:center;
	margin: 0 0 0 0;
	
}
p.flex-caption {
	margin: 0;
	
}

.flex-next {
	overflow: visible;
}
.flex-direction-nav a {
	overflow: visible;
}
.flex-next:link {
	text-decoration: none;	
	color: #004080;
}
.flex-next:hover {
	text-decoration: none;
	font-weight:bold;
	color: #004080;
}
.flex-next:active {
	text-decoration: none;
	color: #004080;
}
.flex-next:active {
	text-decoration: none;
	color: #004080;
}
.flex-prev {
	text-decoration: none;
	color: #004080;
}
.flex-prev:hover {
	text-decoration: none;
	font-weight:bold;
	color: #004080;
}
.more {
	text-decoration: none;	
	color: #8080FF;
}
.more:hover {
	text-decoration: none;	
	color: #0080C0;
}
li.css a {
	border-radius: 0;
}
ul.bullet {
	margin-left: 50px;
	list-style-type: disc;
	font-size: 120%;
}
div.flexslider {
	border: 4px solid #323232;
	background-color: #444;
}
.slider { 
	margin: 20px 0 10px!important; 
}
div.contributors {
	background:url(/customizations/global/pages/homepage_resources/browse-contributors.png) no-repeat;
	/*filter:alpha(opacity=100);*/
	height:150px;
	opacity:1;
	width:150px;
	display: inline-block;
	filter: 		grayscale(0%) brightness(100%);
    -webkit-filter: grayscale(0%) brightness(100%);
    -moz-filter:    grayscale(0%) brightness(100%);
    -ms-filter:     grayscale(0%) brightness(100%);
    -o-filter:      grayscale(0%) brightness(100%);
}
