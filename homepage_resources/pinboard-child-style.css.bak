#restrict-msg:hover {
	color: purple;
	text-decoration: none;	
}
#restrict-msg:visited {
	color: #337ab7;
	text-decoration: none;	
}
#restrict-msg:link {
	color: #337ab7;	
	text-decoration: none;
}
.centerBlock {
    display: table;
    margin: 0 auto;
}
#footer {
	text-align: center;
}
#sponsors {
	background-color: white;
}